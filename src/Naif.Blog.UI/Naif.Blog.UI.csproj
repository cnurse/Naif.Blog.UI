<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <ProductVersion>2.3.0</ProductVersion>
    <FileVersion>2.3.0.5</FileVersion>
    <NuspecFile>$(MSBuildProjectName).nuspec</NuspecFile>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Naif.Blog.Core" Version="3.1.0-beta.3" />
    <PackageReference Include="Naif.Blog.Razor" Version="2.0.0-alpha.6" />
    <PackageReference Include="Naif.Core" Version="3.3.0-beta.4" />
  </ItemGroup>

</Project>
