<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <ProductVersion>2.3.3</ProductVersion>
    <FileVersion>2.3.3.0</FileVersion>
    <NuspecFile>$(MSBuildProjectName).nuspec</NuspecFile>
    <LangVersion>latestmajor</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Naif.Auth0" Version="2.1.0" />
    <PackageReference Include="Naif.Blog.Core" Version="3.2.0-beta-1" />
    <PackageReference Include="Naif.Blog.Razor" Version="2.0.2-alpha-2" />
    <PackageReference Include="Naif.Core" Version="3.3.1" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Naif.Blog.UI.nuspec~" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="ViewModels" />
  </ItemGroup>

</Project>
