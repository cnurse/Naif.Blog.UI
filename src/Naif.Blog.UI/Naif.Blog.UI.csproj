<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <ProductVersion>2.3.2</ProductVersion>
    <FileVersion>2.3.2.3</FileVersion>
    <NuspecFile>$(MSBuildProjectName).nuspec</NuspecFile>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Naif.Blog.Core" Version="3.1.2-alpha.2" />
    <PackageReference Include="Naif.Blog.Razor" Version="2.0.1-alpha.2" />
    <PackageReference Include="Naif.Core" Version="3.3.1-beta.1" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Naif.Blog.UI.nuspec~" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="ViewModels" />
  </ItemGroup>

</Project>
